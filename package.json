    "db:seed": "supabase db seed",
    "db:test": "supabase test",
    "prepare": "husky install",
    "postbuild": "next-sitemap",
    "clean": "rimraf .next coverage playwright-report .jest-cache",
    "validate": "npm run type-check && npm run lint && npm run format:check",
    "docker:build": "docker build -t cpe-connect-app .",
    "docker:run": "docker run -p 3000:3000 cpe-connect-app",
    "pre-commit": "lint-staged"
